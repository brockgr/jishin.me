<h1><%= @city.name_ja %></h1>

<div id="timeplot" style="height: 150px;" data-src="<%= plot_city_url(:format => :json) %>" class="intensity"></div>

<table>
<% @city.intensities.each do |intensity| %>
  <tr>
    <td><%= intensity.time %></td>
    <td><%= intensity.value %></td>
  </tr>
<% end %>
</table>


<%= link_to 'Back', cities_path %>
